{
  "name": "filmizyan",
  "version": "1.0.0",
  "description": "Website for films",
  "license": "ISC",
  "author": "middelmatigheid",
  "type": "module",
  "main": "frontend.js",
  "scripts": {
    "install-deps": "npm install",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "dev:frontend": "concurrently \"npm run backend\" \"npm run frontend:watch\"",
    "backend": "cd backend && python -m uvicorn backend:api --reload --port 5000",
    "frontend": "cd frontend && node frontend.js",
    "frontend:watch": "cd frontend && nodemon frontend.js",
    "frontend:debug": "cd frontend && nodemon --inspect frontend.js",
    "start": "npm run dev",
    "start:watch": "npm run dev:frontend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.11"
  },
  "dependencies": {
    "busboy": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "nunjucks": "^3.2.4",
    "winston": "^3.19.0"
  }
}
